local GuiLibrary = loadstring(game:HttpGet("YOUR_SCRIPT_URL"))()
local window = GuiLibrary.new("My Enhanced GUI")
window.ScreenGui.Parent = game.Players.LocalPlayer.PlayerGui

-- Create tabs
local mainTab = window:AddTab("Main")
local combatTab = window:AddTab("Combat")
local settingsTab = window:AddTab("Settings")

-- Add elements to main tab
local mainSection = mainTab:CreateSection("Welcome")
mainSection:AddLabel("Welcome to My GUI", {
    TextSize = 16,
    Height = 24
})

mainSection:AddParagraph("This is an enhanced GUI with tab support and multiple element types. You can organize your features across different tabs for better user experience.")

local actionsSection = mainTab:CreateSection("Actions")
actionsSection:AddButton("Teleport Home", function()
    print("Teleporting home!")
end)

-- Add elements to combat tab
local combatSection = combatTab:CreateSection("Auto Features")
local autoJumpToggle = combatSection:AddToggle("Auto Jump", false, function(state)
    _G.AutoJumpEnabled = state
    if state then
        spawn(function()
            local player = game:GetService("Players").LocalPlayer
            while _G.AutoJumpEnabled and wait(0.5) do
                if player.Character then
                    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                    if humanoid and humanoid.FloorMaterial ~= Enum.Material.Air then
                        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end
            end
        end)
    end
end)

-- Add elements to settings tab
local settingsSection = settingsTab:CreateSection("Preferences")
settingsSection:AddSlider("Volume", 0, 100, 50, function(value)
    print("Volume set to:", value)
end)
